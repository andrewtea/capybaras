{
    "format_version": "1.10.0",
    "minecraft:client_entity": {
        "description": {
            "identifier": "capybara:capybara",
            "materials": {
                "default": "entity_alphatest"
            },
            "textures": {
                "default": "textures/entity/capybara"
            },
            "geometry": {
                "default": "geometry.capybara"
            },
            "render_controllers": ["controller.render.capybara"],
            "enable_attachables": true,
            "held_item_ignores_lighting": true,
            "spawn_egg": {
				"texture": "capybara:spawn_egg",
				"texture_index": 0
			},
            "animations": {
                "look_at_target": "animation.common.look_at_target",
                "walk": "animation.capybara.walk",
                "idle": "animation.capybara.idle",
                "controller": "controller.animation.capybara"
            },
            "scripts": {
                "animate": [
                    "controller",
                    "look_at_target"
                ]
            }
        }
    }
}
